<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_coin_graph"
    app:startDestination="@id/my_coin">
    <fragment
        android:id="@+id/my_coin"
        android:name="com.example.fragment.module.user.fragment.MyCoinFragment"
        android:label="MyCoinFragment"
        tools:layout="@layout/fragment_my_coin">
        <action
            android:id="@+id/action_coin_to_rank"
            app:destination="@id/coin_rank" />
    </fragment>
    <fragment
        android:id="@+id/coin_rank"
        android:name="com.example.fragment.module.user.fragment.CoinRankFragment"
        android:label="CoinRankFragment"
        tools:layout="@layout/fragment_coin_rank" />
</navigation>